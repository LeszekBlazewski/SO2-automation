{
  "add": {
    "GLOBAL_CAPABILITIES": {
      "permissions": {
        "checks-administrateCheckers": {
          "rules": {
            "Administrators": {
              "action": "ALLOW",
              "added": true
            },
            "Non-Interactive Users": {
              "action": "ALLOW",
              "added": true
            }
          },
          "added": true
        }
      }
    },
    "refs/heads/*": {
      "permissions": {
        "label-Code-Review": {
          "rules": {
            "Non-Interactive Users": {
              "min": "-1",
              "max": "1",
              "action": "ALLOW",
              "added": true,
              "modified": true
            }
          }
        },
        "label-Verified": {
          "rules": {
            "Non-Interactive Users": {
              "min": -1,
              "max": 1,
              "action": "ALLOW",
              "added": true
            },
            "Administrators": {
              "min": -1,
              "max": 1,
              "action": "ALLOW",
              "added": true
            }
          },
          "added": true,
          "label": "Verified"
        }
      }
    },
    "refs/*": {
      "permissions": {
        "read": {
          "rules": {
            "Non-Interactive Users": {
              "action": "ALLOW",
              "added": true
            }
          }
        }
      }
    }
  },
  "remove": {
    "refs/heads/*": {
      "permissions": {
        "label-Code-Review": {
          "rules": {
            "Non-Interactive Users": {}
          }
        }
      }
    }
  }
}
